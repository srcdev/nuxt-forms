{
  "name": "srcdev-nuxt-forms",
  "type": "module",
  "version": "2.3.0",
  "main": "nuxt.config.ts",
  "scripts": {
    "clean": "rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output",
    "cleanall": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output && rm -rf .playground/node_modules && rm package-lock.json",
    "reinstall": "rm -rf node_modules && npm install",
    "cleaninstall": "npm run clean && npm run reinstall",
    "dev": "nuxi dev .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "postinstall": "nuxt prepare .playground",
    "release": "release-it",
    "test": "vitest"
  },
  "files": [
    "assets/",
    "components/",
    "composables/",
    "types/"
  ],
  "devDependencies": {
    "@nuxt/eslint-config": "0.7.5",
    "@nuxt/icon": "1.10.3",
    "@nuxt/test-utils": "3.15.1",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.18.0",
    "happy-dom": "15.11.7",
    "nuxt": "3.15.0",
    "release-it": "18.1.1",
    "typescript": "5.7.3",
    "vitest": "2.1.8",
    "vue": "3.5.13"
  },
  "dependencies": {
    "@iconify-json/carbon": "1.2.5",
    "@iconify-json/gridicons": "1.2.2",
    "@iconify-json/material-symbols": "1.2.12",
    "@iconify-json/material-symbols-light": "1.2.12",
    "@iconify-json/ph": "1.2.2",
    "@iconify-json/radix-icons": "1.2.2",
    "@iconify-json/ri": "1.2.5",
    "modern-normalize": "3.0.1",
    "zod": "3.24.1"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
