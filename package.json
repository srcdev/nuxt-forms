{
  "name": "srcdev-nuxt-forms",
  "type": "module",
  "version": "6.1.23",
  "main": "nuxt.config.ts",
  "scripts": {
    "clean": "rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output",
    "cleanall": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output && rm -rf .playground/node_modules && rm package-lock.json",
    "reinstall": "rm -rf node_modules && npm install",
    "cleaninstall": "npm run clean && npm run reinstall",
    "dev": "nuxi dev .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "postinstall": "nuxt prepare .playground",
    "release": "release-it",
    "test": "vitest"
  },
  "files": [
    "app/",
    "shared/",
    "nuxt.config.ts"
  ],
  "devDependencies": {
    "@iconify-json/carbon": "1.2.14",
    "@iconify-json/gridicons": "1.2.4",
    "@iconify-json/material-symbols": "1.2.44",
    "@iconify-json/material-symbols-light": "1.2.44",
    "@iconify-json/radix-icons": "1.2.5",
    "@iconify-json/ri": "1.2.6",
    "@nuxt/eslint": "1.10.0",
    "@nuxt/icon": "2.1.0",
    "@nuxt/test-utils": "3.20.1",
    "@pinia/nuxt": "0.11.3",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.39.1",
    "happy-dom": "20.0.10",
    "nuxt": "4.2.1",
    "patch-package": "8.0.1",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "release-it": "19.0.6",
    "typescript": "5.9.3",
    "vitest": "3.2.0",
    "zod": "4.1.12"
  },
  "dependencies": {
    "@iconify-json/ph": "1.2.2",
    "modern-normalize": "3.0.1"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
